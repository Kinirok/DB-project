<Button>
    font_size: 24
    background_normal:''
    background_color: (0.5,0.35,0.4,1)
#<MDIconButton>
#    size_hint:(0.1,0.1)
<MDRoundFlatButton>
    font_size: 16
    #background_normal:''
    #background_color: (0.5,0.35,0.4,1)
<InputText>
    multiline: False
    pos_hint: {"center_x": 0.5}
    size_hint: (None,None)
    height:35
    width:250
MDFloatLayout:
	MDScreenManager:
		id:manager
		LoginPage:
			id:logPage
			name:"login"
			login:login
			password:password
			canvas.before:
				Color:
					rgba:(107/255,107/255,107/255,0.42)
				Rectangle
					pos:self.pos
					size:self.size
			BoxLayout:
				orientation: "vertical"
				size: root.width,root.height
				padding: 30
				spacing: 20
				Label:
					background_color: (107/255,107/255,107/255,0)
					color:(94/255,240/255,72/255,0.94)
					outline_width:3
					outline_color:(244/255,217/255,19/255,0.96)
					font_size:72
					bold:True
					italic: True
					text:"DBMusic"
					canvas.before:
						Color:
							rgba: self.background_color
						Rectangle:
							size: self.size
							pos:self.pos

				MDTextField:
					id: login
					hint_text: "Логин"
					icon_right: "account"
					size_hint: (None,None)
					width: 250
					height:35
					pos_hint:{"center_x":0.5}
				MDTextField:
					id:password
					hint_text: "Пароль"
					icon_right: "eye-off"
					size_hint: (None,None)
					width: 250
					height:35
					pos_hint:{"center_x":0.5}
					password: True

				MDRoundFlatButton:
					id:logBut
					text: "Войти"
					on_press:
						if root.ids.logPage.logIn(): root.ids.manager.current="main"
					text_color: 91/255, 240/255, 108/255, 0.94
					pos_hint: {"center_x": 0.5}
				MDRoundFlatButton:
					text:"Зарегистрироваться"
					on_press:root.ids.manager.current="reg"
					text_color: 78/255, 145/255, 240/255, 0.94
					pos_hint: {"center_x": 0.5}
		RegPage:
			name:"reg"
			id:regPage
			reg_login:reg_login
			reg_pass:reg_pass
